{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tureng",
  "title": "Tureng",
  "description": "Quickly look up words using Tureng.com dictionary",
  "icon": "command-icon.png",
  "author": "hzengin",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "lookup",
      "title": "Lookup",
      "description": "Lookup words using Tureng",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.3",
    "@raycast/utils": "1.4.15",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3",
    "ts-md5": "^1.3.1"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}